        pragma solidity ^0.8.18;

        contract Chall1Metacrafters {

            uint myFavNum;
            string myCompleteName;
            bool TD = true;
            address myWallet;

        function setMetacrafters(uint _Age, string memory _myFirstName, address _EthAddress )public{
                myFavNum = _Age;
                myCompleteName = _myFirstName;
                myWallet = _EthAddress;
        }

        function getFavNum() public view returns (uint) {
                 return myFavNum;
        }

        function getCompleteName() public view returns (string memory) {
                 return myCompleteName;
        }

        function getTest() public view returns (bool) {
                 return TD;
        }

        function getAddress() public view returns (address){
                 return myWallet;
        }

} 
